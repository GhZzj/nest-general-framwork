DATABASE_URL="mysql://root:123456@localhost:3306/testdb?"

MYSQL_HOST="localhost"
MYSQL_PORT=3306
MYSQL_DATABASE="testdb"
MYSQL_USER="root"
MYSQL_PASSWORD="123456"

#JWT 秘钥
JWT_SECRET=YOUR-JWT-SECRET
JWT_EXPIRES_IN=1d

