MYNAME=John
PASSWORDS=123456
PORT=3000

#日志存储配置
LOG_ON=true
#MongoDB日志存储配置
LOG_ON_MONGODB=true
LOG_MONGO_DB=mongodb://root:123456@localhost:27017/nest-logs
LOG_MONGO_CONNECTION=logs #日志集合名称
LOG_MONGO_LEVEL=error
LOG_MONGO_CAPPED=true #是否开启集合限制
LOG_MONGO_CAPPED_SIZE=104857600 #集合大小限制
LOG_MONGO_CAPPED_MAX=1000 #集合最大文档数
LOG_MONGO_STORE_HOST=true #是否存储主机信息


